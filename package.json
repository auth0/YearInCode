{
  "name": "nebula",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:api": "run-s api:*",
    "dev:client": "next",
    "api:elasticmq": "docker-compose up --build -d",
    "api:server": "cross-env NODE_ENV=test serverless offline",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE_BUNDLE=true next build",
    "start": "next start",
    "start:ci": "run-p dev:api start",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|tsx|json)\"",
    "test": "is-ci test:ci test:watch",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start:ci http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "0.16.0",
    "@fontsource/inter": "^4.0.1",
    "@middy/input-output-logger": "^1.5.0",
    "@middy/sqs-json-body-parser": "^1.5.0",
    "@middy/sqs-partial-batch-failure": "^1.5.0",
    "@octokit/rest": "^18.0.12",
    "@react-aria/button": "^3.3.0",
    "@svgr/webpack": "^5.5.0",
    "auth0": "^2.31.0",
    "axios": "^0.21.1",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "clsx": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.0",
    "keen-slider": "^5.3.5",
    "middy": "^0.36.0",
    "next": "10.0.4",
    "next-seo": "^4.17.0",
    "nprogress": "^0.2.0",
    "ow": "^0.21.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-query": "^3.5.5",
    "react-query-devtools": "^2.6.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.4",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.11",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/auth0": "^2.20.12",
    "@types/aws-lambda": "^8.10.68",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.19",
    "@types/node": "14.14.16",
    "@types/react": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "autoprefixer": "^9",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cross-env": "^7.0.3",
    "cypress": "^6.2.0",
    "dotenv": "^8.2.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "faker": "^5.1.0",
    "husky": "4.3.6",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.6.3",
    "lint-staged": "10.5.3",
    "msw": "^0.24.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7",
    "postcss-nested": "4.2.3",
    "prettier": "^2.2.1",
    "prettier-plugin-tailwind": "^2.2.8",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^17.0.1",
    "serverless": "^2.16.1",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-functions-base-path": "^1.0.32",
    "serverless-offline": "^6.8.0",
    "serverless-offline-sqs": "^4.1.1",
    "serverless-webpack": "^5.3.5",
    "start-server-and-test": "^1.11.6",
    "storybook-css-modules-preset": "^1.0.5",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tailwindcss-debug-screens": "^2.0.0",
    "ts-loader": "^8.0.12",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "utility-types": "^3.10.0",
    "webpack-node-externals": "^2.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
